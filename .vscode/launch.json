{
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "API lambda-python3.9:MainFunction (python3.9)",
      "invokeTarget": {
        "target": "api",
        "templatePath": "${workspaceFolder}/lambda-python3.9/template.yaml",
        "logicalId": "MainFunction"
      },
      "api": {
        "path": "/test",
        "httpMethod": "get",
        "payload": {
          "json": {}
        }
      },
      "lambda": {
        "runtime": "python3.9"
      }
    }
  ]
}
